PREFIX model: <http://uni-ko-ld.de/ist/model#>
PREFIX prod: <http://uni-ko-ld.de/ist/product#>
PREFIX proc: <http://uni-ko-ld.de/ist/process#>
PREFIX res: <http://uni-ko-ld.de/ist/resource#>
PREFIX prop: <http://uni-ko-ld.de/ist/property#>


proc:SensorFlashing001 model:hasInputProduct prod:radarSensor.
proc:SensorFlashing001 model:hasResource res:firmwareFlasher. 
proc:SensorFlashing001 model:hasMeasurement prop:firmwareVersion.
proc:SensorFlashing001 model:hasConstraint prop:firmwareVersion.
proc:SensorFlashing001 model:hasOutputProduct prod:flashedRadarSensor.
proc:SensorFlashing001 model:hasNextProcess proc:CaseAssembly001.
#
proc:CaseAssembly001 model:hasInputProduct prod:flashedRadarSensor.
proc:CaseAssembly001 model:hasResource res:placer.
proc:CaseAssembly001 model:hasResource res:screwer.
proc:CaseAssembly001 model:hasMeasurement prop:position.
proc:CaseAssembly001 model:hasMeasurement prop:force.
proc:CaseAssembly001 model:hasConstraint prop:position.
proc:CaseAssembly001 model:hasConstraint prop:force.
proc:CaseAssembly001 model:hasOutputProduct prod:assembledSensor.
#
proc:CaseAssembly001 model:hasNextProcess proc:UltrasonicCaseSealing001.
#
proc:UltrasonicCaseSealing001 model:hasNextProcess proc:QualityControl001.
